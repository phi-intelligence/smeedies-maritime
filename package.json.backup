  "scripts": {
    "dev": "vite --config vite.static.config.ts",
    "build": "vite build --config vite.static.config.ts",
    "preview": "vite preview --config vite.static.config.ts",
    "build:aws": "npm run build && aws s3 sync static-build/ s3://agilent-maritime-static --delete",
    "deploy": "npm run build && aws s3 sync static-build/ s3://agilent-maritime-static --delete && aws cloudfront create-invalidation --distribution-id YOUR_DISTRIBUTION_ID --paths '/*'",
    "upload-assets": "node scripts/upload-assets-to-s3.js",
    "setup-aws": "npm run upload-assets && npm run build",
    "setup-aws-infrastructure": "node scripts/setup-aws-quick.js"
  },
